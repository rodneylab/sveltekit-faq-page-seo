    "@sveltejs/kit": "next",
    "commitlint": "^17.0.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "postcss-html": "^1.4.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "^26.0.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2"
